{
  "name": "set-a-title",
  "version": "1.0.0",
  "description": "Set a description",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules ./src/index.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/**/*.test.ts --timeout 15000",
    "test:base": "mocha -r ts-node/register -r tsconfig-paths/register --recursive --timeout 15000",
    "commit": "npx git-cz",
    "lint": "eslint --ext .js,.ts .",
    "lint-fix": "eslint --fix src/**/*.ts",
    "docker:build": "docker build -t node-app .",
    "docker:run": "docker run -p 3000:3000 node-app"
  },
  "engines": {
    "node": "20"
  },
  "keywords": [
    "POS",
    "Services",
    "Transactions"
  ],
  "author": "Juan Duque",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/chai": "^4.3.9",
    "@types/express": "^4.17.20",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "@types/sinon": "^10.0.20",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "chai": "^4.3.10",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "sinon": "^17.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
